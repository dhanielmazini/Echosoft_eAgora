<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.eagora.echosoft.eagora.TelaDeRedirecionamentoMainActivity"
        tools:showIn="@layout/activity_tela_de_redirecionamento_main">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:scrollIndicators="end"
        >


        <ImageButton
            android:id="@+id/eventbtn"
            android:layout_width="match_parent"
            android:layout_height="146dp"
            android:background="#fff"
            android:scaleType="centerCrop"
            android:visibility="visible"
            app:srcCompat="@drawable/balada"
            tools:layout_editor_absoluteX="188dp"
            tools:layout_editor_absoluteY="66dp" />


        <ImageButton
            android:id="@+id/bar_e_restaurante"
            android:layout_width="match_parent"
            android:layout_height="146dp"
            android:background="#fff"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/bar"
            tools:layout_editor_absoluteX="70dp"
            tools:layout_editor_absoluteY="237dp"
            android:layout_marginTop="20dp"/>


        <ImageButton
            android:id="@+id/pontos_turisticos"
            android:layout_width="match_parent"
            android:layout_height="146dp"
            android:background="#fff"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/turistico"
            tools:layout_editor_absoluteX="236dp"
            tools:layout_editor_absoluteY="310dp"
            android:layout_marginTop="20dp"/>

    </LinearLayout>





</android.support.v4.widget.NestedScrollView>
